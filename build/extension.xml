<extension xmlns="http://ns.adobe.com/air/extension/16.0">
  <id>com.freshplanet.Crashlytics</id>
  <versionNumber>1.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libCrashlytics.a</nativeLibrary>
            <initializer>CrashlyticsInitializer</initializer> 
            <finalizer>CrashlyticsFinalizer</finalizer>
        </applicationDeployment>
    </platform>
    <platform name="Android-ARM"> 
        <applicationDeployment> 
            <nativeLibrary>libCrashlytics.jar</nativeLibrary> 
            <initializer>com.freshplanet.ane.crashlytics.Extension</initializer> 
            <finalizer>com.freshplanet.ane.crashlytics.Extension</finalizer> 
        </applicationDeployment> 
    </platform>
    <platform name="Android-x86"> 
        <applicationDeployment> 
            <nativeLibrary>libCrashlytics.jar</nativeLibrary> 
            <initializer>com.freshplanet.ane.crashlytics.Extension</initializer> 
            <finalizer>com.freshplanet.ane.crashlytics.Extension</finalizer> 
        </applicationDeployment> 
    </platform>
    <platform name="default"> 
        <applicationDeployment/> 
    </platform> 
  </platforms>
</extension>